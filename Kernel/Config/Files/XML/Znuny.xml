                <Item Key="Target"></Item>
                <Item Key="PopupType"></Item>
                <Item Key="ClusterName" Translatable="1">Miscellaneous</Item>
                <Item Key="ClusterPriority">999</Item>
            </Hash>
        </Value>
    </Setting>
